{
  "name": "@mancuoj/hooks",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@8.15.4",
  "description": "Collection of common React hooks",
  "author": {
    "name": "mancuoj",
    "url": "https://github.com/mancuoj"
  },
  "license": "MIT",
  "homepage": "https://github.com/mancuoj/hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mancuoj/hooks.git"
  },
  "bugs": "https://github.com/mancuoj/hooks/issues",
  "keywords": [
    "react",
    "typescript",
    "hooks"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint . && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --coverage",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish"
  },
  "peerDependencies": {
    "react": "^16.8.0  || ^17 || ^18"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "bumpp": "^9.4.0",
    "bunchee": "^4.4.8",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "react": "^18.2.0",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  }
}
